{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "target": "ES6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@img/*": ["./src/img/*"],
      "@pages/*": ["./src/pages/*"],
      "@components/*": ["./src/components/*"],
      "@utils/*": ["./src/utils/*"],
      "@api/*": ["./src/api/*"],
      "@styles/*": ["./src/styles/*"],
      "@interfaces/*": ["./src/interfaces/*"]
    }
  }
}